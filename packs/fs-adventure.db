identified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"1","units":"minute"},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":"touch"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"util","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":5,"school":"nec","components":{"vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":true},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"prepared","prepared":true},"scaling":{"mode":"none","formula":""}},"effects":[],"sort":300000,"ownership":{"default":0,"dBogmx4vpeLG8Oap":3},"flags":{"core":{"sourceId":"Item.Vq7JJW4usnvHrvDY"},"midi-qol":{"effectActivation":false},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"rollOther":false,"critOther":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false,"ignoreTotalCover":false,"autoFailFriendly":false,"autoSaveFriendly":false,"offHandWeapon":false},"exportSource":{"world":"csi-waterdeep","system":"dnd5e","coreVersion":"10.291","systemVersion":"2.1.5"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1669086854901,"modifiedTime":1682123133984,"lastModifiedBy":"dBogmx4vpeLG8Oap"},"_id":"g4WU0PU7lupNKFFa"},{"folder":"PEB53pK7kn0VaLhx","name":"Deathvision (Crown)","type":"spell","img":"icons/magic/death/skull-humanoid-white-red.webp","system":{"description":{"value":"<p>You experience the last minute of the life of the creature whose dead body (or part thereof) you touch, through its eyes, ears and sense of touch. This spell fails if it has been used on the body within the last 10 days.</p><p class=\"western\">If you spend the full minute concentrating on the vision you will experience the death of the target. Roll a DC 15 Charisma check. On failure you will suffer 3d6 psychic damage.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"1","units":"minute"},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":"touch"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"charges","target":"jYZE48TYXnklD4qf","amount":3},"ability":"","actionType":"util","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":5,"school":"nec","components":{"vocal":true,"somatic":true,"material":false,"ritual":false,"concentration":true},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"atwill","prepared":true},"scaling":{"mode":"none","formula":""}},"effects":[],"sort":250000,"ownership":{"default":0,"dBogmx4vpeLG8Oap":3},"flags":{"core":{"sourceId":"Item.Vq7JJW4usnvHrvDY"},"midi-qol":{"effectActivation":false},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"rollOther":false,"critOther":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false,"ignoreTotalCover":false,"autoFailFriendly":false,"autoSaveFriendly":false,"offHandWeapon":false},"exportSource":{"world":"csi-waterdeep","system":"dnd5e","coreVersion":"10.291","systemVersion":"2.1.5"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1669086854901,"modifiedTime":1682123134967,"lastModifiedBy":"dBogmx4vpeLG8Oap"},"_id":"Jwa3sodtf4ppLk7G"},{"name":"Speak with Dead (Crown)","ownership":{"default":0,"dBogmx4vpeLG8Oap":3},"type":"spell","system":{"description":{"value":"<p>You grant the semblance of life and intelligence to a corpse of your choice within range, allowing it to answer the questions you pose. The corpse must still have a mouth and can't be undead. The spell fails if the corpse was the target of this spell within the last 10 days.</p><p>Until the spell ends, you can ask the corpse up to five questions. The corpse knows only what it knew in life, including the languages it knew. Answers are usually brief, cryptic, or repetitive, and the corpse is under no compulsion to offer a truthful answer if you are hostile to it or it recognizes you as an enemy. This spell doesn't return the creature's soul to its body, only its animating spirit. Thus, the corpse can't learn new information, doesn't comprehend anything that has happened since it died, and can't speculate about future events.</p>","chat":"","unidentified":""},"source":"SRD 5.1","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"10","units":"minute"},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":10,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"charges","target":"","amount":1},"ability":"","actionType":"util","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":3,"school":"nec","components":{"vocal":true,"somatic":true,"material":true,"ritual":false,"concentration":false},"materials":{"value":"Burning incense","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"atwill","prepared":false},"scaling":{"mode":"none","formula":""}},"sort":500000,"flags":{"core":{"sourceId":"Compendium.dnd5e.spells.I2LUSF5ogc7Bj62e"}},"img":"icons/magic/control/fear-fright-shadow-monster-green.webp","effects":[],"folder":"PEB53pK7kn0VaLhx","_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1661787234108,"modifiedTime":1682534574320,"lastModifiedBy":"dBogmx4vpeLG8Oap"},"_id":"kSbcoAvB3l7ToZ2p"}],"journal":[{"name":"The Flying Squad","pages":[{"sort":200000,"name":"Example Characters","type":"text","_id":"6whwN2ZZudLhtTxx","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>Dragonborn Fighter: @UUID[Actor.Gr0qGXdM3JkTzsaV]{Rhogar}</p><p>Rhogar was a bounty hunter who tracked down wanted criminals for a fee. After a few years of that he decided a steady income and regular hours were preferable to staking out tenements in the dead of winter.</p><p>Half Orc Paladin: @UUID[Actor.KtCG62FBuA1toaCf]{Sutha}</p><p>Sutha's father was a weapons merchant who traveled far and wide. He and his wife were murdered by a rival, and Sutha's uncle took her in and raised her. She joined the city watch, motivated by her desire to find justice for her father, whose murderer is still at large.</p><p>Dwarf Cleric: @UUID[Actor.BaCJ0JGl5Zz8xFE6]{Tordek}</p><p>Tordek is a cleric who made a living brewing ale and presiding over weddings (and providing the refreshments for the receptions).</p><p>Drow Bard: @UUID[Actor.f4TVWwtyBWE41fKU]{Elven Bard}</p><p>Vadania performed in taverns across the city, and stopped quite a few brawls before they got serious. She kept an ear and eye open, and let the city watch know when she heard or saw something untoward in the offing.</p><p>Human Rogue: @UUID[Actor.oH7mkI2zyt2Seywa]{Human Rogue}</p><p>Ælfric was a private investigator who was born in poverty but was adopted by a wealthy family who ran over his mother with their carriage when they were being pursued by a group of assassins. After their escape the wife insisted that they go back and tend to the woman. She was dead, but Ælfric, less than a year old, was bundled up in a blanket in her arms, still very, very quiet. And he still is.</p><p>Human Wizard: @UUID[Actor.M6j4VgEEqfZHf6kV]{Human Wizard}</p><p>Gennis is an evocation wizard who is also an artist. He can draw sketches of suspects after interviewing witnesses.</p><p>Half-elven Sorcerer: @UUID[Actor.SzJMAKwuKppou129]{Naivara}</p><p>Naivara was a merchant who owned a small curio and magic shop that was torched by a jealous rival. </p><p>Half-Elf Fighter/Cleric/Thief: @UUID[Actor.r8BUf0tfWy6fAxqR]{Mindy}</p><p>Mindy started her career with her brother as a guard on caravans. Over time he got into smuggling, and as a good sister she had to join him. She learned a lot about the seamy underside of life, but it troubled her greatly. She frequently went to a temple and prayed for guidance, and eventually saw the light.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":300000,"name":"Acknowledgments","type":"text","_id":"4ouNpz62jXSWrOoV","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>Maps created with assets from Dungeondraft and assets from Forgotten Adventures.</p><p>Some character portraits and art created with Bing/create and PlaygroundAI.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":118750,"name":"DM Notes","type":"text","_id":"7Hg5FH7pHR3YmPAL","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>This setting is intended for experienced D&amp;D 5th edition characters (starting at fifth level). These adventures can be dropped into an existing campaign where existing fifth-level characters are recruited into the squad, or you can create new characters at fifth level, or you can use the sample investigators below.</p><p><em>The Flying Squad was inspired by the London police unit of the same name.</em></p><h2>Location</h2><p class=\"western\">Adventures will take place in and around the city; there will be no long-distance travel.</p><p class=\"western\">This means that players can come and go each week without having to worry about their characters having to travel 200 miles to catch up with the rest of the squad. The @UUID[Item.yv2YbVDxIIo7x4CS]{Amulet of Sending}<span style=\"font-style:normal\"> each investigator is provided with allows easy communication between party members, and the @UUID[Item.5aMlGUpGX3h0Xlc1]{Cloak of Flight} can quickly reunite them.</span></p><p class=\"western\">The exact city is left open to allow you to place the flying squad in any campaign. Waterdeep makes a good base of operations, but any large city in any world will do.</p><h2>First Case</h2><p>The first case for the squad is @UUID[JournalEntry.74x8sFNKT2zFyQ5S]{The Case of the Butchered Butcher}<em>. </em>This murder leads to a linked series of cases, which can be installed from the Flying Squad Adventure compendium as needed.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":0},"flags":{}},{"sort":125000,"name":"Character Backgrounds","type":"text","_id":"znMKpMdpJSnmgHWX","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>All the standard D&amp;D 5e backgrounds are available, but the following backgrounds can be chosen to provide the requisite skill proficiencies.</p><p>@UUID[Item.TfzlrP7Fkz1qCGe5]{Artist}</p><p>@UUID[Item.K6eBNGphhSvyjfVv]{Bounty Hunter}</p><p>@UUID[Item.NnM1lONBMrN96n20]{City Watch}</p><p>@UUID[Item.EswWVWCpBxx5xRKG]{Informant}</p><p>@UUID[Item.zDQHxdyJfNkdWdQt]{Locksmith}</p><p>@UUID[Item.FOW5j9yLze1oSwYm]{Merchant}</p><p>@UUID[Item.HG2DsY4WlP5GCJD6]{Private Investigator}</p><p></p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":112500,"name":"Introduction","type":"text","_id":"AGKvoWfiD5DPYxK3","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>You are walking down a street when you see a notice posted on a wall:</p><div style=\"font-size:16pt\"><p style=\"text-align:center\"><span style=\"font-family:Modesto Condensed\">WANTED:</span></p><p style=\"text-align:center\"><span style=\"font-family:Modesto Condensed\">WIZARDS, WARLOCKS, SORCERERS, CLERICS, DRUIDS, <br />MONKS, BARDS, PALADINS, FIGHTERS, RANGERS<br />and, lamentably, ROGUES of <strong>impeccable</strong> character <br />to assist the city watch in the investigation of crimes</span></p><p style=\"text-align:center\"><span style=\"font-family:Modesto Condensed\">Apply to Capt. Larkspur at the Palace</span></p><p style=\"text-align:center\"><span style=\"font-family:Modesto Condensed\">NO BARBARIANS NEED APPLY!</span></p></div><p class=\"western\">Being at loose ends and in need of a career change, and considering yourself a person of <em>impeccable</em> character, you decide to apply to the City Watch.</p><p class=\"western\">After an intense interview and magical vetting by a paladin, you are inducted into the City Watch. After your initial training period you are assigned to an elite unit of the City Watch, the Flying Squad, formed to investigate crimes that have heretofore been unsolveable<em>.</em></p><p class=\"western\">Your squad will be assigned cases, frequently murders, that have stumped the regular City Watch patrols.</p><p class=\"western\">You have jurisdiction across the entire city, rather than being assigned to a specific ward or precinct.</p><p class=\"western\">And yes, the Flying Squad can actually fly with their @UUID[Item.5aMlGUpGX3h0Xlc1]{Cloaks of Flight}.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":121875,"name":"Background Information","type":"text","_id":"8q8xsYh9tGeWefJ7","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>There has been a rash of murders in which the victims’ hearts and tongues (or heads) have been removed, making successful casting of <em>raise dead</em> and <em>speak with dead</em> spells impossible, and drastically increasing the difficulty of solving these crimes.</p><p>The head of the City Watch commissioned the construction of a special item, the @UUID[Item.1BzXP6tRiZpoEGLP]{Crown of Communing}, to circumvent these limitations, and formed a special unit, the Flying Squad, to use it.</p><p class=\"western\">Because of their special duties, these investigators may come from a wider variety of backgrounds than regular members of the Watch.</p><p>Jealous members of the regular City Watch sometimes refer to members of the Flying Squad as spooks or spookies, because of their black uniforms, because they look like wraiths when they take flight, and because they frequently speak to the dead.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":123438,"name":"Character Creation","type":"text","_id":"rg0dy2ax42hIuO33","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<ul><li><p class=\"initial\">You can be any character class and race except barbarian.</p></li><li><p class=\"western\">You will start at level 5 (6,500 experience points).</p></li><li><p class=\"western\">You can choose any non-chaotic, non-evil alignment (NG, LG, N, or LN).</p></li><li><p class=\"western\">You must have a class or background skill proficiency in at least one of the following: Insight, Intimidation, Investigation, Perception or Survival (for tracking).</p></li><li><p class=\"western\">There should be at least one cleric and one wizard character in the group, to have your magical bases covered.</p></li></ul><h1>Rank and Responsibilities</h1><p class=\"western\">One of your characters will be the ostensible leader of your squad, and will assume the rank of @UUID[Item.Xc4xCD1NF7LNm1kn]{lieutenant} in the City Watch. The lieutenant must have Charisma or Wisdom of 14 or higher.</p><p class=\"western\">The others will hold the rank of @UUID[Item.HQD6BYU1qoyXRm30]{constable} or @UUID[Item.VlMDvH8cQ9ZXHx4E]{sergeant}, at your choice. A sergeant must have Charisma or Wisdom of 12 or higher, and a constable 10 or higher.</p><p>Your squad's commanding officer has his office at the palace, and his rank is captain (Charisma or Wisdom of 16 or higher required). </p><p>Officers who display leadership and bravery may win promotions, though service of at least two years in a rank is usually required before a promotion is granted.</p><p>You wear your insignia of rank on your cloak.</p><table><tbody><tr><td><img src=\"modules/flying-squad/images/objects/constable.jpg\" alt=\"Constable\" width=\"128\" height=\"128\" /></td><td><img src=\"modules/flying-squad/images/objects/sergeant.jpg\" alt=\"Sergeant\" width=\"128\" height=\"128\" /></td><td><img src=\"modules/flying-squad/images/objects/lieutenant.jpg\" alt=\"Lieutenant\" width=\"128\" height=\"128\" /></td><td><img src=\"modules/flying-squad/images/objects/captain.jpg\" alt=\"Lieutenant\" width=\"128\" height=\"128\" /></td></tr><tr><td>Constable</td><td>Sergeant</td><td>Lieutenant</td><td>Captain</td></tr></tbody></table><p class=\"western\">One character (a spellcaster) must become attuned with the @UUID[Item.1BzXP6tRiZpoEGLP]{Crown of Communing}.</p><h1>Hooks, Bonds, Flaws</h1><p class=\"western\">From the lists below, pick one or more positive character hooks (taken as a bond), each balanced by one negative character hook (taken as a flaw). You can also make up your own hooks. Provide names and brief descriptions of any people mentioned. You should avoid choosing the same hook as another player.</p><h2>Positive Character Hooks</h2><ul><li><p class=\"bullet-entry\">A relative or friend runs a shop in the market.</p></li><li><p class=\"bullet-entry\">A relative or friend is in the city’s army.</p></li><li><p class=\"bullet-entry\">A relative or friend is in the City Watch.</p></li><li><p class=\"bullet-entry\">You are related to a noble family.</p></li><li><p class=\"bullet-entry\">You are an orphan and were adopted by a wealthy merchant.</p></li><li><p class=\"bullet-entry\">You have a well-known reputation as a celebrated member of your class.</p></li><li><p class=\"bullet-entry\">People constantly mistake you for a member of the nobility and want to be your <em>friend</em>.</p></li><li><p class=\"bullet-entry\">People just like to talk to you.</p></li></ul><h2>Flaws</h2><ul><li><p class=\"bullet-entry\">You are prone to alcoholic blackouts.</p></li></ul><ul><li><p class=\"bullet-entry\">A half-brother or -sister is a wanted criminal; your relationship is unknown to the authorities.</p></li><li><p class=\"bullet-entry\">You are having a secret affair with the spouse of an important city official.</p></li><li><p class=\"bullet-entry\">Your family has a secret history of dragon/demon/outsider worship.</p></li><li><p class=\"bullet-entry\">You are universally disliked by an entire class of merchants, or thieves, traders, blacksmiths, woodworkers, tavern keepers, etc.</p></li><li><p class=\"bullet-entry\">You have made an enemy of an important person in the city.</p></li><li><p class=\"bullet-entry\">People always pick fights with you.</p></li><li><p class=\"bullet-entry\">People constantly mistake you for someone else (a criminal, vagabond or con man) and want to beat you to a bloody pulp.</p></li><li><p class=\"bullet-entry\">People are always giving you the cold shoulder.</p></li><li><p class=\"bullet-entry\">Dead bodies creep you out.</p></li></ul><p class=\"western\"></p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":162500,"name":"Equipment","type":"text","_id":"9tE5w7BIco1y6Iax","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p class=\"initial\">The City Watch will provide each investigator with the following:</p><ul><li><p class=\"bullet-entry\">Your choice of nonmagical armor and weapons.</p></li><li><p class=\"bullet-entry\">Any equipment or materials required for your investigations.</p></li><li><p class=\"bullet-entry\">Flying Squad uniform: black tunic and a @UUID[Item.5aMlGUpGX3h0Xlc1]{Cloak of Flight}, with faint silver wings on the back.</p></li><li><p class=\"bullet-entry\"><span style=\"text-decoration:none\"><em>@UUID[Item.yv2YbVDxIIo7x4CS]{Amulet of Sending}</em></span></p></li><li><p class=\"bullet-entry\">Monthly salaries. Constable: 100 gp, Sergeant: 120 gp, Lieutenant: 150 gp, Captain: 200 gp</p></li><li><p class=\"bullet-entry\">Most of your assignments will take place in the city, so there won’t be a need for field rations, camping gear, etc.</p></li></ul><p class=\"western\">One of you will be entrusted with a @UUID[Item.1BzXP6tRiZpoEGLP]{Crown of Communing}, which you must protect with your very life.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":181250,"name":"Experience Awards","type":"text","_id":"PWCWHSeV8BGW9S3P","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>In addition to gaining experience from defeating foes, experience will be gained from capturing perpetrators and putting them in jail, as well as using your deductive powers.</p><p class=\"western\">Experience can be gained by making <em><strong>deductions</strong></em>. This is equal to the experience gained by defeating a creature whose Challenge Rating is your current level. For example, at level 5 you will make 1,800 XP for a correct deduction.</p><p class=\"western\">Thus, you will gain experience for deducing who committed a crime, and then again for defeating or putting the perpetrator behind bars.</p><p class=\"western\">When you make a deduction submit it in writing to the DM. For example: “Deduction: It was the cook in the kitchen with a rolling pin,“ or “Deduction: the victim was actually a spy for Glorfindale.” At the end of the adventure experience for correct deductions will be added to experience for defeating/capturing perpetrators.</p><p>Alternately, characters can gain a level after each major case is solved.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":122657,"name":"Department Policies","type":"text","_id":"1PlkGNzNSSY9ySZ2","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>The following policies apply to the duties and conduct of Flying Squad investigators.</p><p>The officer overseeing the squad's performance, Capt. Broam Larkspur, is a meticulous paladin and stickler for the rule of law and humane treatment of suspects.</p><h3 class=\"western\">Civil Forfeiture</h3><p class=\"initial\">Evidence, assets and equipment used in the commission of a crime are subject to forfeiture by the squad. These items are to be kept in the armory of the squad’s office pending the outcome of criminal proceedings. If perpetrators are found guilty in a court of law the forfeiture is permanent.</p><p class=\"western\">These items can be used by the squad in pursuit of their duties. Cash and valuables seized from suspects apprehended in the commission of a crime are to be deposited in the strongbox in the armory. These funds may be used for squad expenses; for example, to pay for the casting of spells to raise suspects from the dead if they should suffer fatal wounds during their apprehension.</p><h3 class=\"western\">Use of Force</h3><p class=\"initial\">Squad members should use non-lethal means in pursuit of suspects if possible. Injury to bystanders and damage to civilian property should be avoided at all costs.</p><p class=\"western\">Medical and magical aid should be rendered to all injured humanoid suspects so that they may appear at trial.</p><h3 class=\"western\">Use of Magic</h3><p class=\"initial\">Magic may not be used to coerce confessions from suspects. Spells such as <em>zone of truth</em><span style=\"font-style:normal\"> may be used to help determine the veracity of testimony.</span></p><p class=\"western\"><span style=\"font-style:normal\">Spells such as </span><em>charm person</em><span style=\"font-style:normal\"> and </span><em>suggestion </em><span style=\"font-style:normal\">may be used to gain the trust and cooperation from suspects, but charmed suspects cannot be held liable for any statements made or actions performed while under the effects of any mind control magic.</span></p><h3>Testimony and Trials</h3><p>If a case goes to trial officers may need to testify at court. Most cases do not result in a trial, as criminals who admit their guilt under <em>zone of truth </em>usually do not protest their innocence and instead cooperate with the authorities, turning on their co-conspirators to reduce their sentences.</p><h3>Accountability</h3><p>Investigators may be subjected to <em>zone of truth</em> while testifying in court. Deviation from department policies can range from temporary suspension, reduction in rank, loss of pay, dismissal, and in severe cases, prosecution and imprisonment upon conviction.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":171875,"name":"Magic Items and Spells","type":"text","_id":"Zbmvo9Picz4hD5ZH","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<h1>Magic Items</h1><p>The following magic items are available to the Flying Squad.</p><p>@UUID[Item.yv2YbVDxIIo7x4CS]{Amulet of Sending}</p><p>@UUID[Item.5aMlGUpGX3h0Xlc1]{Cloak of Flight}</p><p>@UUID[Item.1BzXP6tRiZpoEGLP]{Crown of Communing}</p><h1>New Spells</h1><p>The following new spells are available to clerics, wizards, sorcerers and warlocks.</p><p>@UUID[Item.bRsvq6LxZtywzgLa]{Commune with Dead}</p><p>@UUID[Item.g4WU0PU7lupNKFFa]{Deathvision}</p><h1>Wizard Spells</h1><p>As part of their rigorous training for the Flying Squad wizards are taught the following defensive, infiltration, influencing, information, and crowd control spells, in addition to any they pick up on their own:</p><p>@UUID[Compendium.dnd5e.spells.eS7XnnApoxRxYXPs]{Charm Person}</p><p>@UUID[Compendium.dnd5e.spells.cg50KpBkBdPK6vPL]{Clairvoyance}</p><p>@UUID[Compendium.dnd5e.spells.ghXTfe7sgCbgf1Q8]{Detect Magic}</p><p>@UUID[Compendium.dnd5e.spells.ppWAAEul0QHtm4er]{Detect Thoughts}</p><p>@UUID[Compendium.dnd5e.spells.A3q2gTNqG6fvNGrv]{Disguise Self}</p><p>@UUID[Compendium.dnd5e.spells.XXUDGFELgoskdOD0]{Fear}</p><p>@UUID[Compendium.dnd5e.spells.CKZTpZlxj7hjjo2H]{Mage Armor}</p><p>@UUID[Compendium.dnd5e.spells.aU62xVUBYkAQWIHv]{Nondetection}</p><p>@UUID[Compendium.dnd5e.spells.XZhdgVK3cLoxNCQl]{Remove Curse}</p><p>@UUID[Compendium.dnd5e.spells.z1mx84ONwkXKUZd7]{Shield}</p><p>@UUID[Compendium.dnd5e.spells.KhwiSi9fwVfUPtku]{Sleep}</p><p>@UUID[Compendium.dnd5e.spells.DQzlB5Y3k791W5bH]{See Invisibility}</p><p>@UUID[Compendium.dnd5e.spells.TwlD4PLcltv7Xh7j]{Stinking Cloud}</p><p>@UUID[Compendium.dnd5e.spells.zMAWdyc8UVb37BK4]{Suggestion}</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":123048,"name":"The Station","type":"text","_id":"A3UpW4vrDHvnEnUa","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p class=\"initial\">The Flying Squad station is not far from the palace, near fine eating and drinking establishments.</p><img src=\"modules/flying-squad/maps/Office-numbered.webp\" /><h3 class=\"western\">1. Reception</h3><p class=\"initial\">This area has a desk for your receptionist, a half-elf constable named Mindy, and four chairs. There is a bulletin board near the door with various notices, wanted posters and a city map.</p><p>Mindy keeps the station in order, making sure that there are plenty of arrows in the armory, that the books in the library are properly shelved, that any prisoners are properly fed and secured, that Rendel has everything spic and span, etc.</p><p>If the members of the squad need anything, Mindy is always on call and can usually be reached with the <em>amulet of sending</em>. She will respond immediately, even if she is tranced.</p><p class=\"western\">The door is made of thick iron-bound wood and can be barred from the inside.</p><h3 class=\"western\">2. Bullpen</h3><p class=\"initial\">The bullpen for the sergeants and constables. There are six desks and chairs, with bookcases for each.</p><h3 class=\"western\">3. Lieutenant’s Office</h3><p class=\"initial\">This is the office of the lead inspector in charge of the Flying Squad.</p><p class=\"western\">There is a desk with a chair, a bookcase.</p><h3 class=\"western\">3a. Interrogation Room</h3><p class=\"western\">There is a table with six chairs around it.</p><p class=\"western\">A barred window overlooks the street.</p><h3 class=\"western\">4. Library</h3><p class=\"initial\">Book cases line the walls of this room. There are two tables with four chairs each. A card catalog is near the door.</p><p class=\"western\">The library is stocked with all the standard works on arcane lore, history, law; most everything that a researcher would need to help solve a case.</p><p class=\"western\">A barred window overlooks the street.</p><h3 class=\"western\">5. Armory</h3><p class=\"initial\">This room is stocked with two of every nonmagical weapon. There is a barrel of arrows and a smaller one of crossbow bolts, which are replenished on a regular basis by Mindy.</p><p class=\"western\">The iron door of the armory is locked with <em>arcane lock</em>, with the password “swordfish.”</p><h3 class=\"western\">6. Holding Cells</h3><p class=\"initial\">There are four holding cells here for suspects who are brought in. Each cell has two cots, a sink and a chamber pot.</p><p class=\"western\">The cells are separated from each other by brick walls and from the holding cell entry area by bars. The cell doors are bars that swing open, and have an opening where food can be passed in and prisoners can extend their hands so they can be manacled before the cell door is opened.</p><p>The turnkey is Rendel. He feeds and waters any prisoners and cleans up around the station.</p><p class=\"western\">Prisoners are usually held here until transfer is arranged to the main city jail.</p><p class=\"western\">A locked cabinet holds 10 pairs of wrist manacles and 10 pairs of ankle manacles. Another cabinet holds water and simple rations for the prisoners.</p><h3 class=\"western\">7. Lavatory</h3><p class=\"initial\">There is a sink and pitcher of water to wash up with.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}},{"sort":120313,"name":"Personalizing the Campaign","type":"text","_id":"cWXtTAKnLr7GuBmj","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>D&amp;D campaigns that directly engage the players are more fun than ones where the player characters are just on the outside looking in. A good way of doing this is giving the player character a stake in the world or some kind of connection to the non-player characters in the adventure.</p><p>If the players are using the example characters provided here, there are natural crossover points. For example:</p><ul><li><p>Sutha the half-orc paladin has a friend or sister who is a member of the Red Claws.</p></li><li><p>Vadania the drow bard regularly performed at the infamous Midnight Sun tavern in the seediest part of town.</p></li><li><p>Naivara the sorcerer once had an affair with Abelard Mott, a wealthy man who is found dead. She is unexpectedly named the heir to his fortune. His estranged wife accuses Naivara of seducing, charming and killing him!</p></li><li><p>Ælfric the rogue's wealthy adoptive parents are implicated in a murder.</p></li></ul><p>If you are dropping these adventures into an existing campaign, you may wish to substitute the player characters' family or acquaintances of the player characters for the victims or perpetrators of the events herein.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":0},"flags":{}},{"sort":121094,"name":"Increasing Challenge Ratings","type":"text","_id":"UOJDDlcpspYDoeBt","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p>Depending on the order of the leads the players follow up on, you may need to increase the challenge rating of the NPC encounters. The easiest way is to increase the number of creatures in the encounters (add an extra ghoul or two to the ghoul encounter, increase the size of the gang at Snakhar's hideout in The Pits, give Findizier another Renfieldesque familiar, and so forth).</p><p>If the characters drastically outlevel the named NPCs (such as Findizier or Snakhar), increase their Challenge Rating. The guidelines in the Dungeon Master's Guide for calculating CR can be helpful, but here are some easy ways:</p><ul><li><p>Increase their AC, by adding better armor or a shield, increasing Dexterity by 2, etc.</p></li><li><p>Increase hit points, by changing them directly, or by increasing Constitution</p></li><li><p>Increase the damage of attacks by increasing strength or a weapon that does more damage</p></li><li><p>Add higher-level spells for spellcasters </p></li><li><p>Add additional features (see below)</p></li><li><p>Replace them with a similar type of monster with an appropriately higher Challenge Rating.</p></li></ul><p>For example, Findizier starts out as a Vampire Spawn with the added Shapechanger feature, but if the squad doesn't meet her until they are level 8, she may have developed the Charm action and the Misty Escape and Spider Climb features (from the CR 13 @UUID[Compendium.dnd5e.monsters.DDO9lDCDtNHkfShP]{Vampire}). To grant her these powers:</p><ul><li><p>Click the @UUID[Compendium.dnd5e.monsters.DDO9lDCDtNHkfShP]{Vampire} link.</p></li><li><p>Click the Import button in the Vampire window header and close that window.</p></li><li><p>Find the Vampire in the Actor tab of the Journal and click to open that window.</p></li><li><p>Find Findizier in the Actor tab and click to open her window.</p></li><li><p>Drag and drop the desired actions from the Vampire Features tab to Findizier's Features tab.</p></li><li><p>Increase the Challenge Rating (CR) in the upper right corner of the Findizier's character sheet.</p></li></ul><p>Other monsters can be found in the \"Monsters (SRD)\" compendium under the Actor heading in the Compendium tab.</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":0},"flags":{}}],"folder":"t4OYJx9NxJtOfun3","sort":100000,"ownership":{"default":2,"dBogmx4vpeLG8Oap":3},"flags":{},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1682107582780,"modifiedTime":1682706132628,"lastModifiedBy":"dBogmx4vpeLG8Oap"},"_id":"WT36xTPIdYhmg2hD"},{"folder":"t4OYJx9NxJtOfun3","name":"Bulletin Board","pages":[{"sort":100000,"name":"Timon Tanglefoot","type":"text","_id":"ffiXMlLgsqN2ARaw","title":{"show":true,"level":1},"image":{},"text":{"format":1,"content":"<p style=\"font-size:16pt;text-align:center\">WANTED FOR ESPIONAGE, TRESPASS</p><img src=\"modules/flying-squad/images/portraits/Timon.jpg\" width=\"300\" height=\"300\" class=\" centered\" /><p>Name: Timon Tanglefoot, alias Timorous Timon</p><p>Description: Human, age 27, 5'9\", 150 lbs, shaggy brown hair, pale skin, brown eyes</p><p>Identifying Marks: none known</p><p>Known Associates: Spymaster Sammel, Mank (\"Crybaby\")</p>","markdown":""},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"ownership":{"default":-1},"flags":{}}],"sort":0,"ownership":{"default":2,"dBogmx4vpeLG8Oap":3},"flags":{},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1682687410355,"modifiedTime":1682688435979,"lastModifiedBy":"dBogmx4vpeLG8Oap"},"_id":"d7LgQdmXcKDWDzix"}],"scenes":[{"name":"Flying Squad Station","active":true,"navigation":true,"navOrder":-100000,"navName":"","background":{"src":"modules/flying-squad/maps/Office-numbered.webp","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"foreground":null,"foregroundElevation":20,"thumb":"world